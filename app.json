{
  "expo": {
    "name": "netflax",
    "slug": "netflax",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/netflix-sigle.png",
    "scheme": "netflax",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/netflix-sigle.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/netflix-sigle.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/netflix-sigle.png"
    },
    "plugins": [
      "expo-router",
      [
      "expo-font",
        {
          "fonts": [
            "./assets/fonts/Rubik-Regular.ttf",
            "./assets/fonts/Rubik-Bold.ttf",
            "./assets/fonts/Rubik-Medium.ttf",
            "./assets/fonts/Rubik-Light.ttf"
          ],
          "android": {
            "fonts": [
              {
                "fontFamily": "Rubik-Regular",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/Rubik-Regular.ttf",
                    "weight": "normal",
                    "style": "normal"
                  }
                ]
              }
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
